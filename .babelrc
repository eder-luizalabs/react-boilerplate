{
    "presets": ["es2015", "stage-0", "react"],
    "plugins": [
        ["transform-decorators-legacy"]
    ],
    "env": {
        "production": {
            "plugins": [
                ["babel-plugin-transform-es2015-modules-commonjs"],
                ["babel-plugin-transform-es3-member-expression-literals"],
                ["babel-plugin-transform-es3-property-literals"]
            ]
        }
    }
}
